{
  "name": "nuxt-meilisearch",
  "type": "module",
  "version": "1.3.0",
  "author": "Alex Duval",
  "license": "MIT",
  "homepage": "https://nuxt-meilisearch.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xlanex6/nuxt-meilisearch.git"
  },
  "contributors": [],
  "keywords": [
    "meilisearch",
    "search",
    "vue-instantsearch",
    "nuxt",
    "module"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm lint && bumpp && pnpm publish",
    "test": "vitest run",
    "test:types": "tsc --noEmit",
    "test:watch": "vitest watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@meilisearch/instant-meilisearch": "0.18.1",
    "@nuxt/kit": "3.11.2",
    "instantsearch.css": "8.1.0",
    "meilisearch": "0.40.0",
    "vue-instantsearch": "4.16.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.18.1",
    "@nuxt/eslint-config": "0.3.13",
    "@nuxt/module-builder": "0.6.0",
    "@nuxt/schema": "3.11.2",
    "@nuxt/ui": "2.16.0",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@types/node": "20.12.12",
    "bumpp": "9.4.1",
    "defu": "6.1.4",
    "eslint": "9.3.0",
    "nuxt": "3.11.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "doc": "docs",
    "example": "playground"
  }
}
